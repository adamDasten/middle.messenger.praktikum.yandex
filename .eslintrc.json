{
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"rules": {
		"no-var": "error",
		"@typescript-eslint/no-non-null-assertion": 0,
		"@typescript-eslint/no-explicit-any": 0,
		"@typescript-eslint/no-unused-vars": 2,
		"max-len": [2, 100]
	}
}
